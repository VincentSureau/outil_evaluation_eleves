<form method="post" action="{{ path('tp_delete', {'id': tp.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ tp.id) }}">
    <button class="btn">Supprimer</button>
</form>
